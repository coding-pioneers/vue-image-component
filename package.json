{
  "name": "vue-image-component",
  "author": "Sascha Steinbrink",
  "license": "MIT",
  "version": "0.3.0",
  "description": "A Vue component to lazy load and display images",
  "main": "./dist/vue-image-component.common.js",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --name vue-image-component ./src/components/VueImage.vue",
    "build-bundle": "vue-cli-service build --target lib --name vue-image-component ./src/components/VueImage.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "typescript": "^3.0.0",
    "vue-template-compiler": "^2.5.17"
  }
}
